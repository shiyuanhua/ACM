#include<iostream>
#include<cmath>
using namespace std;

void judge(int a, int b, int c,int n)
{
	for (int i = 10; i <= 100; i++)
	{
		if (i % 3 == a&&i % 5 == b&&i % 7 == c)
		{
			cout << "case" << n << ": "<< i << endl;
			break;
		}
		else if (i == 100)
			cout << "case" << n << ": " << "no answer!" << endl;

	}
}

int main(){
	//韩信点兵
	int a,b,c,k=0,sq[2][3],num=0;
	while (cin >> a >> b >> c)
	{
		sq[k][0] = a;
		sq[k][1] = b;
		sq[k][2] = c;
		k++;
		if (k > 1)
			break;
	}

	for (int i = 0; i < 2; i++)
	{
		num++;
		judge(sq[i][0], sq[i][1], sq[i][2],num);
	}
	
	return 0;
}
